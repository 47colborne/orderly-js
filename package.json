{
  "name": "orderly-js",
  "version": "0.0.3",
  "description": "Simple Ajax Priority Queue with the request versioning and aborting",
  "main": "./dist/index.js",
  "scripts": {
    "integration": "webpack-dev-server --hot --inline --config ./webpack/config.integration.js --content-base ./integration",
    "test": "node_modules/.bin/mocha -c -w test --compilers js:babel-core/register --require babel-polyfill -R progress $(find test -path '*_test*')",
    "watch": "webpack --watch --color --progress --config ./webpack/config.compile.js",
    "compile": "NODE_ENV=compile webpack --config ./webpack/config.compile.js"
  },
  "author": "Yroo Team",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0",
    "node-fetch": "^1.5.3",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "fastpriorityqueue": "^0.1.0"
  }
}
