{
  "name": "orderly-js",
  "version": "1.0.0",
  "description": "A simple javascript prioritize queue",
  "main": "dist/index.js",
  "repository": "https://github.com/47colborne/orderly-js",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha -c -w test --compilers js:babel-core/register --require babel-polyfill -R progress $(find test -path '*.spec*')",
