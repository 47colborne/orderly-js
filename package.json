    "compile": "webpack --env production"
  },
  "author": "Yroo Team",
  "license": "MIT",
  "dependencies": {
    "fastpriorityqueue": "^0.2.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.1",
    "mocha": "^3.4.2",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0",
    "webpack": "^2.6.1"
  }
}
