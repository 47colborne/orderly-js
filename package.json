    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.2",
    "node-fetch": "^1.6.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^1.0.0"
